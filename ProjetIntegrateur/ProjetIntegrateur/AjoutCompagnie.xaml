<Window x:Class="ProjetIntegrateur.AjoutCompagnie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetIntegrateur"
        mc:Ignorable="d"
        Title="Ajouter une compagnie d'assurance " Height="233.8" Width="557.333" WindowStartupLocation="CenterScreen" Closed="Window_Closed">
    <Grid>
        <GroupBox x:Name="groupBox" Header="Entrez les informations de la compagnie d'assurance" HorizontalAlignment="Left" Height="154" Margin="27,29,0,0" VerticalAlignment="Top" Width="493"/>
        <Label x:Name="label" Content="Nom de la compagnie :" HorizontalAlignment="Left" Margin="90,83,0,0" VerticalAlignment="Top" Width="143"/>
        <TextBox x:Name="txtNomComp" HorizontalAlignment="Left" Height="27" Margin="233,86,0,0" BorderThickness="1"  TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
        <Button x:Name="btnAjout" Content="Ajouter" Background="LightCoral" HorizontalAlignment="Left" Margin="316,135,0,0" VerticalAlignment="Top" Width="113" Click="btnAjout_Click"/>
        <Button x:Name="button1" Content="Annuler" Background="LightCoral" HorizontalAlignment="Left" Margin="162,135,0,0" VerticalAlignment="Top" Width="116" Click="button1_Click"/>
    </Grid>
</Window>
